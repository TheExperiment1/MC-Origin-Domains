{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Example Mod",
  "description": "Origin Domains is a mod that incorporates origins specialized for certain terrains, such as the sky or underground.",
  "authors": [
    "TheExperiment1"
  ],
  "contact": {
    "homepage": "https://github.com/TheExperiment1/MC-Origin-Domains",
    "sources": "https://github.com/TheExperiment1/MC-Origin-Domains"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.TheExperiment1.originDomains.ExampleMod"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "origins": "1.9.0",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
